<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<%- include('./partials/head')  %>

<body>
	<!-- Header -->
	<%- include('./partials/headerDeslogeado')  %> 
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
				<form action="GET" action="/users/register">
					<label for="">Email</label>
					<input type="text" email="email">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Usuario</label>
					<input type="text" name="usuario">
				</form>

			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Contraseña</label>
					<input type="password" password="contraseña">
				</form>
			</h2>		
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Fecha de Nacimiento</label>
					<input type="date" date="Fecha de Nacimiento">
				</form>
			</h2>		
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">Nro de documento</label>
					<input type="number" numerico="numerico">
				</form>
			</h2>
			<h2 class="col-12">
				<form action="GET" action="/users/register">
					<label for="">foto de perfil</label>
					<input type="file" img="foto de perfil">
				</form>
			</h2>
			

	<!-- footer -->
	<%- include('./partials/footer')  %> 
	<!-- /footer -->
</body>

</html>